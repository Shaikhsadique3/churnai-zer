{

  "compilerOptions": {
    "lib": ["deno.window", "deno.ns", "dom", "esnext"],
    "types": ["deno.ns", "https://esm.sh/@supabase/supabase-js@2.50.2"],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["./**/*.ts"]
}